[configuration]

entry_symbol = "library_init"
compatibility_minimum = "4.1"
reloadable = false

[libraries]
; Relative paths ensure that our GDExtension can be placed anywhere in the project directory.
macos.single.debug = "bin/macos/libmidi.macos.template_debug.dylib"
macos.double.debug = "bin/macos/libmidi.macos.template_debug.double.dylib"
macos.single.release = "bin/macos/libmidi.macos.template_release.dylib"
macos.double.release = "bin/macos/libmidi.macos.template_release.double.dylib"

ios.arm64.single.debug = "bin/ios/libmidi.ios.template_debug.arm64.dylib"
ios.arm64.double.debug = "bin/ios/libmidi.ios.template_debug.arm64.double.dylib"
ios.arm64.single.release = "bin/ios/libmidi.ios.template_release.arm64.dylib"
ios.arm64.double.release = "bin/ios/libmidi.ios.template_release.arm64.double.dylib"

windows.x86_32.single.debug = "bin/windows/midi.windows.template_debug.x86_32.dll"
windows.x86_32.double.debug = "bin/windows/midi.windows.template_debug.x86_32.double.dll"
windows.x86_32.single.release = "bin/windows/midi.windows.template_release.x86_32.dll"
windows.x86_32.double.release = "bin/windows/midi.windows.template_release.x86_32.double.dll"

windows.x86_64.single.debug = "bin/windows/midi.windows.template_debug.x86_64.dll"
windows.x86_64.double.debug = "bin/windows/midi.windows.template_debug.x86_64.double.dll"
windows.x86_64.single.release = "bin/windows/midi.windows.template_release.x86_64.dll"
windows.x86_64.double.release = "bin/windows/midi.windows.template_release.x86_64.double.dll"

linux.x86_64.single.debug = "bin/linux/libmidi.linux.template_debug.x86_64.so"
linux.x86_64.double.debug = "bin/linux/libmidi.linux.template_debug.x86_64.double.so"
linux.x86_64.single.release = "bin/linux/libmidi.linux.template_release.x86_64.so"
linux.x86_64.double.release = "bin/linux/libmidi.linux.template_release.x86_64.double.so"

linux.arm64.single.debug = "bin/linux/libmidi.linux.template_debug.arm64.so"
linux.arm64.double.debug = "bin/linux/libmidi.linux.template_debug.arm64.double.so"
linux.arm64.single.release = "bin/linux/libmidi.linux.template_release.arm64.so"
linux.arm64.double.release = "bin/linux/libmidi.linux.template_release.arm64.double.so"

linux.rv64.single.debug = "bin/linux/libmidi.linux.template_debug.rv64.so"
linux.rv64.double.debug = "bin/linux/libmidi.linux.template_debug.rv64.double.so"
linux.rv64.single.release = "bin/linux/libmidi.linux.template_release.rv64.so"
linux.rv64.double.release = "bin/linux/libmidi.linux.template_release.rv64.double.so"

android.x86_64.single.debug = "bin/android/libmidi.android.template_debug.x86_64.so"
android.x86_64.double.debug = "bin/android/libmidi.android.template_debug.x86_64.double.so"
android.x86_64.single.release = "bin/android/libmidi.android.template_release.x86_64.so"
android.x86_64.double.release = "bin/android/libmidi.android.template_release.x86_64.double.so"

android.arm64.single.debug = "bin/android/libmidi.android.template_debug.arm64.so"
android.arm64.double.debug = "bin/android/libmidi.android.template_debug.arm64.double.so"
android.arm64.single.release = "bin/android/libmidi.android.template_release.arm64.so"
android.arm64.double.release = "bin/android/libmidi.android.template_release.arm64.double.so"

web.wasm32.single.debug = "bin/web/libmidi.web.template_debug.wasm32.nothreads.wasm"
web.wasm32.double.debug = "bin/web/libmidi.web.template_debug.wasm32.double.nothreads.wasm"
web.wasm32.single.release = "bin/web/libmidi.web.template_release.wasm32.nothreads.wasm"
web.wasm32.double.release = "bin/web/libmidi.web.template_release.wasm32.double.nothreads.wasm"

[icons]
MIDI = "res://addons/godot-midi/icons/MIDI.svg"
AudioStreamMIDI = "res://addons/godot-midi/icons/AudioStreamMIDI.svg"
VirtualKeyboard = "res://addons/godot-midi/icons/VirtualKeyboard.svg"
